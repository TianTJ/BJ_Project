            IDX = 3
        }else if((135<=pos.x && pos.x<225) && (225>=pos.y && pos.y>135)){
            IDX = 4
        }else if((-215<=pos.x && pos.x<-135) && (135>=pos.y && pos.y>45)){
            IDX = 5
        }else if((-135<=pos.x && pos.x<-45) && (135>=pos.y && pos.y>45)){
            IDX = 6
