
    get_pos_ection:function(pos){
        let IDX
        if((-225<=pos.x && pos.x<-135) && (225>=pos.y && pos.y>135)){
            IDX = 0
        }else if((-135<=pos.x && pos.x<-45) && (225>=pos.y && pos.y>135)){
            IDX = 1
        }else if((-45<=pos.x && pos.x<45) && (225>=pos.y && pos.y>135)){
            IDX = 2
